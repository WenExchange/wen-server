{
  "kind": "collectionType",
  "collectionName": "exchange_users",
  "info": {
    "singularName": "exchange-user",
    "pluralName": "exchange-users",
    "displayName": "ExchangeUser"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "string"
    },
    "maker_nonce": {
      "type": "integer",
      "default": 0
    },
    "hash_nonce": {
      "type": "integer",
      "default": 0
    },
    "request_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request-log.request-log",
      "mappedBy": "exchange_user"
    },
    "batch_signed_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::batch-signed-order.batch-signed-order",
      "mappedBy": "exchange_user"
    }
  }
}
