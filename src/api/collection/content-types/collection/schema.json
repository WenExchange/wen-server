      "unique": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "token_uri": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "logo_url": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "banner_url": {
      "type": "string",
      "required": true
    },
    "twitter": {
      "type": "string",
      "required": false
    },
    "discord": {
      "type": "string"
    },
    "website": {
      "type": "string"
    },
    "nfts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::nft.nft",
      "mappedBy": "collection"
    },
    "collection_stat_logs": {
      "type": "relation",
