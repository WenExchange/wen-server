    },
    "floor_price": {
      "type": "biginteger"
    },
    "volume_24h": {
      "type": "biginteger",
      "default": "0",
      "required": false
    },
    "volume_7d": {
      "type": "biginteger",
      "default": "0",
      "required": false
    },
    "change_24h": {
      "type": "float"
    }
